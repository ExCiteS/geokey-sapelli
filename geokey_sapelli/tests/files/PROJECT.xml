<?xml version="1.0" encoding="UTF-8"?>
<SapelliCollectorProject id="1111" name="Mapping Cultures" version="1.1">
  <Configuration>
    <!-- data transmission settings go here -->
  </Configuration>
  <Form id="Horniman Gardens">
    <Choice id="Garden Feature" rpws="2" cols="2">
		<Choice value="Flowers" img="flowers.png" rows="3" cols="2">
			<Choice value="Red Flowers" img="red flowers.png"/>
			<Choice value="Blue Flowers" img="blue flowers.png"/>
			<Choice value="Yellow Flowers" img="yellow flowers.png"/>
			<Choice value="Edible Plants" img="BeenTold.png"/>
			<Choice value="Medicinal Plants" img="Medicine.png"/>
		</Choice>
		<Choice value="Animals" img="Sheep.png" rows="2" cols="1">
			<Choice value="Two Legged Animal" img="Chicken.png"/>
			<Choice value="Four Legged Animal" img="Sheep.png"/>
		</Choice>
		<Choice value ="Bench" img="new bench.png" rows="2" cols="1">
			<Choice value="Old Bench" img="old bench.png" rows="2" cols="1">
				<Choice value="Old Bench With Memorial" img="memorial.png"/>
				<Choice value="Old Bench with No Memorial" img="no memorial.png"/>
			</Choice>
			<Choice value="New Bench" img="new bench.png" rows="2" cols="1">
				<Choice value="New Bench With Memorial" img="memorial.png"/>
				<Choice value="New Bench with No Memorial" img="no memorial.png"/>
			</Choice>
		</Choice>
		<Choice value="Bin" img="bins.png" rows="3" cols="1">
			<Choice value="Covered Bin" img="covered bin.png"/>
			<Choice value="Uncovered Bin" img="uncovered bin.png"/>
			<Choice value="Dog Bin" img="dog bin.png"/>
		</Choice>
	</Choice>

    <Location id="Position" type="GPS" timeout="120" optional="true"/>
    <Choice id="Confirmation" noColumn="true" showCancel="false" showForward="false">
      <Choice img="ok.svg" alt="Confirm" showBack="false" showCancel="false" showForward="false">
        <Choice img="restart.svg" alt="Another" jump="_SAVE+LOOPFORM"/>
        <Choice img="exit.svg" alt="Exit" jump="_SAVE+EXITAPP"/>
      </Choice>
      <Choice img="cancel.svg" alt="Discard" jump="_LOOPFORM"/>
    </Choice>
  </Form>

</SapelliCollectorProject>
